{
    "name": "modera/backend-google-analytics-bundle",
    "type": "symfony-bundle",
    "description": "Provides support for gathering 'pageview' analytics in backend using Google Analytics.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Lissovski"
        },
        {
            "name": "Sergei Vizel"
        }
    ],
    "require": {
        "php": ">=8.1",
        "modera/config-bundle": "^5.0|^6.0",
        "modera/expander-bundle": "^5.0|^6.0",
        "modera/foundation-bundle": "^5.0|^6.0",
        "modera/mjr-integration-bundle": "^5.0|^6.0",
        "modera/security-bundle": "^5.0|^6.0",
        "modera/server-crud-bundle": "^5.0|^6.0"
    },
    "require-dev": {
        "phake/phake": "^4.0",
        "phpunit/phpunit": "^10.5",
        "symfony/symfony": "^6.0"
    },
    "autoload": {
        "psr-4": { "Modera\\BackendGoogleAnalyticsBundle\\": "" }
    },
    "autoload-dev": {
        "classmap": [
            "Tests/Fixtures/DummyKernel.php"
        ]
    },
    "extra": {
        "modera-module": {
            "register-bundle": "Modera\\BackendGoogleAnalyticsBundle\\ModeraBackendGoogleAnalyticsBundle"
        },
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
