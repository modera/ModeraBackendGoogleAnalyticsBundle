{
    "name": "modera/backend-google-analytics-bundle",
    "type": "symfony-bundle",
    "description": "Provides support for gathering 'pageview' analytics in backend using Google Analytics.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@modera.org",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "modera/config-bundle": "^3.1|^4.0|^5.0",
        "modera/security-bundle": "^3.1|^4.0|^5.0",
        "modera/foundation-bundle": "^3.1|^4.0|^5.0",
        "modera/mjr-integration-bundle": "^3.1|^4.0|^5.0",
        "sergeil/expander-bundle": "^1.0"
    },
    "require-dev": {
        "phake/phake": "^2.0",
        "phpunit/phpunit": "^5.5",
        "symfony/symfony": "^4.4|^5.0"
    },
    "autoload": {
        "psr-4": { "Modera\\BackendGoogleAnalyticsBundle\\": "" }
    },
    "extra": {
        "modera-module": {
            "register-bundle": "Modera\\BackendGoogleAnalyticsBundle\\ModeraBackendGoogleAnalyticsBundle"
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
